
<div class="form-group <%= required %>">
  <label for="obj-<%= attr %>" class="col-sm-2 control-label"><%= title %></label>
  <div class="col-sm-10">
    <div class="form-control-box">
      <div class="overlay"><i class="fa fa-refresh fa-spin"></i></div>
      <input type="hidden" class="form-control" id="obj-<%= attr %>" name="obj-<%= attr %>">
      <div class="checkbox-list"></div>
      <div class="checkbox-sub sample">
        <div class="checkbox-item"><label title="Full access"><input class="permission" type="checkbox" value="full">Full access</label></div>
        <div class="checkbox-item"><label title="Read"><input class="permission" type="checkbox" value="read">Read</label></div>
        <div class="checkbox-item"><label title="Write"><input class="permission" type="checkbox" value="write">Edit</label></div>
        <div class="checkbox-item"><label title="Delete"><input class="permission" type="checkbox" value="delete">Deletion</label></div>
        <div class="checkbox-item"><label title="Use"><input class="permission" type="checkbox" value="use">Use</label></div>
      </div>
    </div>
    <p class="error-block"></p>
  </div>
</div>
