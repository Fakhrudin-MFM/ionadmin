
<div class="form-group <%= required %>">
  <label for="obj-<%= attr %>" class="col-sm-2 control-label"><%= title %></label>
  <div class="col-sm-10">
    <div class="form-control-box">
      <div class="overlay"><i class="fa fa-refresh fa-spin"></i></div>
      <input type="hidden" class="form-control" id="obj-<%= attr %>" name="obj-<%= attr %>">
      <div class="checkbox-list"></div>
      <div class="checkbox-sub sample">
        <div class="checkbox-item"><label title="Полный доступ"><input class="permission" type="checkbox" value="full">Полный доступ</label></div>
        <div class="checkbox-item"><label title="Чтение"><input class="permission" type="checkbox" value="read">Чтение</label></div>
        <div class="checkbox-item"><label title="Запись"><input class="permission" type="checkbox" value="write">Запись</label></div>
        <div class="checkbox-item"><label title="Удаление"><input class="permission" type="checkbox" value="delete">Удаление</label></div>
        <div class="checkbox-item"><label title="Использование"><input class="permission" type="checkbox" value="use">Использование</label></div>
      </div>
    </div>
    <p class="error-block"></p>
  </div>
</div>
